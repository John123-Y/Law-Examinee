{
  "env": {
    "node": true,
    "es2021": true,
    "mongo": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["mongodb", "@typescript-eslint"],
  "rules": {
    "no-undef": "off",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}